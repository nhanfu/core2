<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <base href="/" />
    <meta charset="utf-8" />
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="theme-color" content="#000000">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="tenant" content="donga" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>Đông Á</title>
    <link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />
    <link asp-append-version="true" href="~/css/new.min.css" rel="stylesheet" />
    <link asp-append-version="true" href="~/css/main.min.css" rel="stylesheet" />
    <link asp-append-version="true" href="~/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/chat.css" asp-append-version="true" />
    <link rel="manifest" href="manifest.json">
    <link rel="apple-touch-icon" href="https://www.gstatic.com/youtube/img/branding/favicon/favicon_144x144.png">
</head>
<body class="layout-horizontal theme-1 bg-light">
    <div class="navbar navbar-expand-md navbar-dark border-top-0 pl-2 pr-0 mode" id="header">
        <div class="container-fluid">
            <div class="d-flex d-xl-none col-3 col-md-1 pl-0">
                <button class="navbar-toggler sidebar-mobile-main-toggle ml-0 d-flex" type="button">
                    <i class="far fa-bars"></i>
                </button>
            </div>
            <div id="menu">
                <div class="sidebar sidebar-dark sidebar-main sidebar-expand-xl d-xl-none">
                    <div class="sidebar-content">
                        <div class="card card-sidebar-mobile">
                        </div>
                    </div>
                </div>
            </div>
            <div class="d-flex d-md-none align-items-center col-7 justify-content-end pr-0">
                <div class="dropdown dropdown-user">
                    <a href="#" class="navbar-nav-link pr-md-0" data-toggle="dropdown" aria-expanded="false"><span class="fas fa-user-circle"></span></a>
                    <div class="dropdown-menu dropdown-menu-right">
                        <a href="/profile/index" class="dropdown-item"><i class="far fa-user"></i>Admin</a><a href="#" class="dropdown-item"><i class="far fa-bell"></i>Thông báo</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item text-danger btn-logout"><i class="far fa-power-off"></i> Đăng xuất</a>
                    </div>
                </div>
            </div>
            <div class="collapse navbar-collapse" id="navbar-mobile">
                <ul class="navbar-nav ml-md-auto ml-xl-auto d-none d-md-flex">
                    <li class="nav-item dropdown d-flex align-items-center" id="user-active">
                    </li>
                    <li class="nav-item dropdown d-flex align-items-center" id="notification-list">
                    </li>
                    <li class="nav-item dropdown dropdown-user  profile-info1">
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="page-content" id="main" autocomplete="off">
        <div class="content-wrapper">
            <div class="container-fluid">
                <div class="page-header">
                    <div class="breadcrumb-line breadcrumb-line-light header-elements-md-inline border-top-0" style="font-size:0.8rem" id="breadcrumb-menu-mode">
                        <div class="d-flex">
                            <div class="card-header p-0 ">
                                <ul class="nav nav-tabs nav-tabs-highlight mb-0 navTabTopFilter" id="tabs"></ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="content" id="tab-content">
                    @RenderBody()
                </div>
            </div>
        </div>
        <div class="center d-none">
            <div class="contacts">
                <i class="fas fa-bars fa-2x"></i>
                <h2>
                    Contacts
                </h2>
                <div id="contacts">
                </div>
            </div>
            <div class="chat">
                <div class="contact bar">
                    <div class="close-chat">
                        <i class="fal fa-times"></i>
                    </div>
                    <div class="pic stark"></div>
                    <div class="name" id="FullNameChat">
                        Tony Stark
                    </div>
                    <div class="seen">
                        Today at 12:56
                    </div>
                </div>
                <div class="messages" id="chat">
                    <div class="time">
                        Today at 11:41
                    </div>
                    <div class="message parker">
                        Hey, man! What's up, Mr Stark? 👋
                    </div>
                    <div class="message stark">
                        Kid, where'd you come from?
                    </div>
                    <div class="message parker">
                        Field trip! 🤣
                    </div>
                    <div class="message parker">
                        Uh, what is this guy's problem, Mr. Stark? 🤔
                    </div>
                    <div class="message stark">
                        Uh, he's from space, he came here to steal a necklace from a wizard.
                    </div>
                    <div class="message stark">
                        <div class="typing typing-1"></div>
                        <div class="typing typing-2"></div>
                        <div class="typing typing-3"></div>
                    </div>
                </div>
                <div class="input">
                    <i class="fas fa-camera"></i><i class="far fa-laugh-beam"></i><input placeholder="Type your message here!" type="text" /><i class="fas fa-microphone"></i>
                </div>
            </div>
        </div>
        <div class="arcontactus-message-button">
            <i class="fal fa-rocketchat"></i>
        </div>
    </div>
    <script asp-append-version="true" src="~/js/jquery-3.5.1.min.js"></script>
    <script asp-append-version="true" src="~/js/bootstrap.bundle.min.js"></script>
    <script asp-append-version="true" src="~/js/pnotify.min.js"></script>
    <script asp-append-version="true" src="~/js/theme1.js"></script>
    <script asp-append-version="true" src="~/js/sweetalert2.js"></script>
    <script asp-append-version="true" src="~/js/bridge.min.js"></script>
    <script asp-append-version="true" src="~/js/bridge.meta.min.js"></script>
    <script asp-append-version="true" src="~/js/tms.newtonsoft.json.js"></script>
    <script asp-append-version="true" src="~/js/Core.js"></script>
    <script asp-append-version="true" src="~/js/Core.meta.min.js"></script>
    <script asp-append-version="true" src="~/js/fix.js"></script>
    <script asp-append-version="true" src="~/js/TMS.UI.min.js"></script>
    <script asp-append-version="true" src="~/js/TMS.UI.meta.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <script>
        $(".close-chat").on("click", () => {
            $(".center").addClass("d-none");
        });
        $(".arcontactus-message-button").on("click", () => {
            $(".center").removeClass("d-none");
        });
    </script>
</body>
</html>