<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <base href="/" />
    <meta charset="utf-8" />
    <meta name="tenant" content="wr1">
    <meta name="startup" content="admin">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>TMS - Truck Management System</title>
    <link rel="shortcut icon" type="image/x-icon" href="/image/logo.png" />
    <link href="css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" href="/theme/OverlayScrollbars.min.css">
    <link rel="stylesheet" href="/theme/adminlte.min.css?v=3.2.0">
    <link href="css/adminite.css" rel="stylesheet" />
    <link href="css/main.css" rel="stylesheet" />
    <link href="css/style.css" rel="stylesheet" />
    <link href="css/zoom-image.css" rel="stylesheet" />
    <link rel="stylesheet" href="css/chat.css" asp-append-version="true" />
</head>
<body class="hold-transition sidebar-mini layout-fixed layout-navbar-fixed layout-footer-fixed">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <ul class="navbar-nav" id="tabs">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
            </ul>
            <ul class="navbar-nav ml-auto">
                <li class="nav-item dropdown" id="user-active">
                </li>
                <li class="nav-item dropdown" id="notification-list">
                </li>
                <li class="nav-item dropdown profile-info1" id="profile-info1">
                </li>
            </ul>
        </nav>
        <aside class="main-sidebar main-sidebar-custom sidebar-light-info elevation-2">
            <a href="/" class="brand-link">
                <img src="/image/logo.png" alt="TMS"
                     class="brand-image" style="opacity: .8">
                <span class="brand-text font-weight-light">TMS</span>
            </a>
            <div class="sidebar">
                <div class="form-inline">
                    <div class="input-group" data-widget="sidebar-search">
                        <input class="form-control form-control-sidebar" type="search" placeholder="Search"
                               aria-label="Search">
                        <div class="input-group-append">
                            <button class="btn btn-sidebar">
                                <i class="fas fa-search fa-fw"></i>
                            </button>
                        </div>
                    </div>
                </div>
                <nav class="mt-2" id="menu">
                </nav>
            </div>
        </aside>
        <div class="content-wrapper" id="tab-content" style="position: relative;">
            @RenderBody()
        </div>
        <aside class="control-sidebar control-sidebar-dark">
        </aside>
    </div>
    <script asp-append-version="true" src="js/jquery-3.5.1.min.js"></script>
    <script src="/js/bootstrap.bundle.min.js"></script>
    <script src="/js/jquery.overlayScrollbars.min.js"></script>
    <script src="/js/adminlte.min.js?v=3.2.0"></script>
    <script src="/js/pnotify.min.js"></script>
    <script asp-append-version="true" src="js/canvasjs.min.js"></script>
    <script asp-append-version="true" src="js/tms.bridge.js"></script>
    <script asp-append-version="true" src="js/newtonsoft.json.js"></script>
    <script asp-append-version="true" src="~/js/bridge.meta.min.js"></script>
    <script asp-append-version="true" src="/js/Core.js"></script>
    <script asp-append-version="true" src="/js/Core.meta.min.js"></script>
    <script asp-append-version="true" src="js/fix.js"></script>
    <script asp-append-version="true" src="js/fix_page_index.js"></script>
    <script asp-append-version="true" src="js/zoom-image.min.js"></script>
    <script asp-append-version="true" src="js/sweetalert2.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>