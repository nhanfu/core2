                            "CanAdd": true,
                            "Query": "{\r\n    \"sql\": \"select * from ApprovalConfig as ds where ParentId = '{ParentId}' and ParentId is not null\",\r\n    \"total\": \"select count(ds.Id) as total from ApprovalConfig as ds where ParentId = '{ParentId}' and ParentId is not null\"\r\n}",
