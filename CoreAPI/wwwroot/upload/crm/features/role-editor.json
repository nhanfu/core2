            "Components": [
                {
                    "Id": "00571d34-0000-0000-8000-05d6e9501dcd",
                    "FieldName": "FeaturePolicy",
                    "Order": 1,
                    "ComponentType": "GridView",
                    "Column": 12,
                    "Row": 500,
                    "GroupBy": "GroupFeatureId",
                    "ClassName": "sticky",
                    "ChildStyle": "height: calc(100vh - 20.5rem)",
                    "Events": "{\r\n    \"dblclick\": \"Edit\"\r\n}",
                    "Active": true,
                    "Query": "{\r\n    \"sql\": \"select ds.*,ds1.ParentId,seq.Id as GroupFeatureId from FeaturePolicy as ds left join Feature ds1 on ds.FeatureId = ds1.Id left join Feature ds2 on ds1.ParentId = ds2.Id outer apply (select top 1 * from FeaturePolicy where FeatureId = ds1.ParentId and RoleId = ds.RoleId) seq where ds.RoleId = '{RoleId}' and ds1.ParentId is not null and seq.Id is not null\",\r\n    \"total\": \"select count(ds.Id) as total from FeaturePolicy as ds left join Feature ds1 on ds.FeatureId = ds1.Id left join Feature ds2 on ds1.ParentId = ds2.Id outer apply (select top 1 * from FeaturePolicy where FeatureId = ds1.ParentId and RoleId = ds.RoleId) seq where ds.RoleId = '{RoleId}' and ds1.ParentId is not null and seq.Id is not null\"\r\n}",
                    "IsRealtime": true,
                    "RefName": "FeaturePolicy",
                    "PreQuery": "{\r\n    \"RoleId\" : this.EditForm.Entity.Id\r\n}",
                    "OrderBy": "ds2.[Order] asc,seq.Id asc",
                    "IsMultiple": true,
                    "ReportTypeId": 1
                }
            ]
        },
