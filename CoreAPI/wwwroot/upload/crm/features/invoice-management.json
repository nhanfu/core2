    "Order": 2,
    "Script": "return {\r\n    AddVatInv: function () {\r\n        var entity = {\r\n            StatusId: 1,\r\n            ActionId: 2,\r\n            VoucherTypeId: 11,\r\n            InsertedBy: this.Token.UserId,\r\n            FeeTypeId: 0,\r\n            TypeId: 2,\r\n            ObhTypeId: 1,\r\n            PostDate: this.dayjs().format('YYYY-MM-DDTHH:mm:ss'),\r\n            InvoiceDate: this.dayjs().format('YYYY-MM-DDTHH:mm:ss'),\r\n            ExchangeRateINV: this.Decimal(1).div(this.Decimal(JSON.parse(localStorage.getItem(\"ExchangeRateUSD\"))[\"VND\"])),\r\n        }\r\n        this.OpenPopup(\"issue-invoice-editor\", entity);\r\n    },\r\n    AddSOA: function () {\r\n        var entity = {\r\n            StatusId: this.SalesFunction[\"APPROVE_SOA\"] ? 3 : 1,\r\n            NoApproved: this.SalesFunction[\"APPROVE_SOA\"] ? true : false,\r\n            VoucherTypeId: 13,\r\n            InsertedBy: this.Token.UserId,\r\n            FeeTypeId: 0,\r\n            TypeId: 4,\r\n            ObhTypeId: 1,\r\n            ActionId: 2,\r\n            PostDate: this.dayjs().format('YYYY-MM-DDTHH:mm:ss'),\r\n            InvoiceDate: this.dayjs().format('YYYY-MM-DDTHH:mm:ss')\r\n        }\r\n        this.OpenPopup(\"soa-editor\", entity);\r\n    },\r\n    EditSOA: function (btn, entity) {\r\n        this.OpenPopup(\"soa-editor\", entity);\r\n    },\r\n    AddDebit: function () {\r\n        var entity = {\r\n            StatusId: 1,\r\n            ActionId: 2,\r\n            VoucherTypeId: 11,\r\n            InsertedBy: this.Token.UserId,\r\n            FeeTypeId: 0,\r\n            TypeId: 2,\r\n            ObhTypeId: 1,\r\n            PostDate: this.dayjs().format('YYYY-MM-DDTHH:mm:ss'),\r\n            InvoiceDate: this.dayjs().format('YYYY-MM-DDTHH:mm:ss'),\r\n            ExchangeRateINV: this.Decimal(1).div(this.Decimal(JSON.parse(localStorage.getItem(\"ExchangeRateUSD\"))[\"VND\"])),\r\n        }\r\n        this.OpenPopup(\"issue-invoice-editor\", entity);\r\n    },\r\n    EditDebit: function (btn, entity) {\r\n        this.OpenPopup(\"issue-invoice-editor\", { Id: entity.Id }, true);\r\n    },\r\n    EditInvoice: function (btn, entity) {\r\n        this.OpenPopup(\"vatinvoice-editor\", { Id: entity.Id }, true);\r\n    },\r\n    EditPayment: function (btn, entity) {\r\n        this.OpenPopup(\"payment-request-editor\", { Id: entity.Id }, true);\r\n    }\r\n}",
