    "Script": "return {\r\n    Add: function () {\r\n        var entity = {\r\n            StatusId: this.SalesFunction[\"APPROVE_PAYMENT_REQUEST\"] ? 3 : 1,\r\n            NoApproved: this.SalesFunction[\"APPROVE_PAYMENT_REQUEST\"] ? true : false,\r\n            VoucherTypeId: 11,\r\n            InsertedBy: this.Token.UserId,\r\n            FeeTypeId: \"ds.TypeId in (1,2,3,4)\",\r\n            TypeId: 2,\r\n            ObhTypeId: 1,\r\n            PostDate: this.dayjs().format('YYYY-MM-DDTHH:mm:ss'),\r\n            InvoiceDate: this.dayjs().format('YYYY-MM-DDTHH:mm:ss'),\r\n            ExchangeRateINV: this.Decimal(JSON.parse(localStorage.getItem(\"ExchangeRateVND\"))[\"USD\"]),\r\n        }\r\n        this.OpenPopup(\"payment-request-editor\", entity);\r\n    },\r\n    Edit: function (btn, entity) {\r\n        this.OpenPopup(\"payment-request-editor\", { Id: entity.Id }, true);\r\n    }\r\n}",
