    "Name": "change-password-editor",
    "Label": "Change Password",
    "Order": 1,
    "Style": "width:500px",
    "Script": "return {\r\n    UpdatePassword: async function (form, gridview) {\r\n        const valid = await this.EditForm.IsFormValid();\r\n        if (!valid) {\r\n            return false;\r\n        }\r\n        const response = await this.Client.PostAsync({ Password: this.Entity.OldPassword, NewPassword: this.Entity.NewPassword }, \"/api/User/UpdatePassword\");\r\n        if (response) {\r\n            this.EditForm.Dirty = false;\r\n            this.Toast.Success(\"Update password success!\")\r\n        }\r\n        else {\r\n            this.Toast.Warning(\"Update password fail!\")\r\n        }\r\n    }\r\n}",
    "Events": "{\r\n    \"onsave\": \"OnSave\",\r\n    \"onsaveadd\": \"OnSaveAdd\"\r\n}",
